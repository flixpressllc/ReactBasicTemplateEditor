{
  "name": "ReactBasicTemplateEditor",
  "private": true,
  "version": "2.5.2-pre",
  "description": "Template Editor for Flixpress.com",
  "main": "",
  "browserslist": [
    "Chrome 49",
    "Firefox 52",
    "ie >= 9",
    "last 4 versions"
  ],
  "scripts": {
    "clean": "rimraf build/* && rimraf dist/*",
    "copy:dist": "copyfiles -f ./build/* ./dist && rimraf dist/*.map",
    "prebuild": "npm run clean",
    "build": "webpack --env=prod",
    "postbuild": "npm run lintBuild",
    "predist": "npm run automated-test",
    "dist": "npm run build && npm run copy:dist",
    "aws": "npm run build && gulp aws",
    "lint": "eslint ./src",
    "lintBuild": "eslint --env browser --plugin compat --rule \"compat/compat:2\" --no-eslintrc --no-ignore ./build",
    "posttest": "npm run lint",
    "beforeRelease": "gulp checkRepoIsClean && npm run dist",
    "release:major": "npm run beforeRelease && gulp release --increment=major",
    "release:minor": "npm run beforeRelease && gulp release --increment=minor",
    "release:patch": "npm run beforeRelease && gulp release --increment=patch",
    "start": "node server.js --env=dev",
    "chrome": "node server.js --env=dev --openWith='google chrome'",
    "test": "node scripts/test.js --env=jsdom",
    "automated-test": "node scripts/test.js --env=jsdom --noWatch"
  },
  "repository": "https://github.com/flixpressllc/ReactBasicTemplateEditor",
  "awsTesting": {
    "bucket": "FlixSamples",
    "path": "development_files/Scripts/templateEditor/"
  },
  "keywords": [],
  "author": "Don Denton",
  "devDependencies": {
    "@types/cropperjs": "^0.0.26",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.3",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.4.7",
    "bower-webpack-plugin": "^0.1.9",
    "breakpoint-sass": "^2.7.0",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.25.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.6",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.7.0",
    "gulp-git": "^2.1.0",
    "gulp-s3": "^0.11.0",
    "jest": "^19.0.2",
    "jquery": "^1.12.4",
    "js-stylesheet": "0.0.1",
    "json-loader": "^0.5.4",
    "jsonfile": "^2.4.0",
    "minimist": "^1.2.0",
    "node-sass": "^3.9.3",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.5.1",
    "react-confirm": "^0.1.14",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-modal": "^1.4.0",
    "react-tabs": "^0.8.3",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.0.2",
    "semver": "^5.3.0",
    "style-loader": "^0.13.1",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0",
    "yargs": "^7.0.2"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "cropperjs": "^1.0.0-rc.3",
    "flux": "^3.1.3",
    "happy-helpers": "^1.0.1",
    "js-beautify": "^1.6.4",
    "jxon": "github:happycollision/jxon#v2.0.0-beta.2",
    "normalize.css": "^4.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-color": "^2.13.5",
    "react-dom": "^15.6.1",
    "react-sortable-hoc": "^0.6.7",
    "react-test-renderer": "^15.6.1",
    "react-textarea-autosize": "^4.3.2",
    "reactcss": "^1.2.2",
    "reset.css": "^2.0.2",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/cfg/jest/FileStub.js",
      "^.+\\.(css|scss|sass)$": "<rootDir>/cfg/jest/CSSStub.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/cfg/polyfills.js",
      "<rootDir>/cfg/bluebirdPromiseRejection.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      ".test.js"
    ],
    "testEnvironment": "jsdom"
  }
}
