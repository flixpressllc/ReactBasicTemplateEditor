{
  "name": "ReactBasicTemplateEditor",
  "private": true,
  "version": "1.1.0",
  "description": "Template Editor for Flixpress.com",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "webpack --env=dist && webpack --env=dist-min",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "chrome": "node server.js --env=dev --openWith='google chrome'",
    "test": "node scripts/test.js --env=jsdom",
    "dev": "webpack --env=flix-dev --watch"
  },
  "repository": "https://github.com/flixpressllc/ReactBasicTemplateEditor",
  "keywords": [],
  "author": "Don Denton",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "bower-webpack-plugin": "^0.1.9",
    "breakpoint-sass": "^2.7.0",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.25.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.0.6",
    "jest": "^17.0.3",
    "jquery": "^1.12.4",
    "js-stylesheet": "0.0.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^3.9.3",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.3.1",
    "react-confirm": "^0.1.14",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-modal": "^1.4.0",
    "react-tabs": "^0.7.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "js-beautify": "^1.6.4",
    "jxon": "github:happycollision/jxon#v2.0.0-beta.1",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-color": "^2.4.0",
    "react-dom": "^15.3.1",
    "react-sortable-hoc": "^0.4.7",
    "reactcss": "^1.0.9",
    "reset.css": "^2.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/cfg/jest/FileStub.js",
      "^.+\\.(css|scss|sass)$": "<rootDir>/cfg/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/cfg/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "jsdom"
  }
}
