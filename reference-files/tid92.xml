
<TConfig strscp="true">
  <ImagePath>Cropped/</ImagePath>
  <ThumbnailPath>TB/</ThumbnailPath>
  <ImagePreviewPath>images/</ImagePreviewPath>
  <AllowStereoscopic>False</AllowStereoscopic>
  <MinimumSlideCount>1</MinimumSlideCount>
  <MaximumSlideCount>1</MaximumSlideCount>
  <MinimumImagesPerSlide>8</MinimumImagesPerSlide>
  <MaximumImagesPerSlide>8</MaximumImagesPerSlide>
  <EnableLogo>false</EnableLogo>
  <TextLinesImagePreviewPath>images/</TextLinesImagePreviewPath>
  <InstructionalVideoUrl>https://mediarobotvideo.s3.amazonaws.com/BasicTemplateWithImagesHelp.mp4</InstructionalVideoUrl>
  <InstructionalText>Drag &amp; drop 8 images into the Images Container, then click EDIT to customize captions.</InstructionalText>
  <ImageCaptions>Caption</ImageCaptions>
  <TemplateCaptions>
    <TemplateCaptionSetting>
      <Label>Begin Text</Label>
    </TemplateCaptionSetting>
    <TemplateCaptionSetting>
      <Label>End Text</Label>
    </TemplateCaptionSetting>
    <TemplateCaptionSetting>
      <Label>Background Color</Label>
      <IsColor>true</IsColor>
    </TemplateCaptionSetting>
   <TemplateCaptionSetting>
      <Label>Transition Type</Label>
      <Options>
        <DropDownOption>
          <Text>Cubes</Text>
          <Value>Cubes</Value>
        </DropDownOption>
		<DropDownOption>
		  <Text>3D Morph</Text>
		  <Value>3D Morph</Value>
		</DropDownOption>
		 <DropDownOption>
		  <Text>Triangle Slash</Text>
		  <Value>Traingle Slash</Value>
		 </DropDownOption>
		  <DropDownOption>
			  <Text>Broken Glass</Text>
			  <Value>Broken Glass</Value>
		  </DropDownOption>
	  </Options>
    </TemplateCaptionSetting>
	  <TemplateCaptionSetting>
		  <Label>Style</Label>
		  <Options>
			  <DropDownOption>
				  <Text>Full Color</Text>
				  <Value>Full Color</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Add Back Color</Text>
				  <Value>Add Back Color</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Back Color Negative</Text>
				  <Value>Back Color Negative</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Back Color Pastel</Text>
				  <Value>Back Color Pastel</Value>
			  </DropDownOption>
		  </Options>
	  </TemplateCaptionSetting>
	  <TemplateCaptionSetting>
		  <Label>Glow</Label>
		  <Options>
			  <DropDownOption>
				  <Text>Enabled</Text>
				  <Value>Enabled</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Disabled</Text>
				  <Value>Disabled</Value>
			  </DropDownOption>
		  </Options>
	  </TemplateCaptionSetting>
	  <TemplateCaptionSetting>
		  <Label>Particle Icon</Label>
		  <Options>
			  <DropDownOption>
				  <Text>None</Text>
				  <Value>None</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Basketball</Text>
				  <Value>basketball19</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Butterfly</Text>
				  <Value>butterfly90</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Money</Text>
				  <Value>coin11</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Earth</Text>
				  <Value>earth53</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Flower</Text>
				  <Value>flower178</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Heart</Text>
				  <Value>heart296</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Leaf</Text>
				  <Value>leaf32</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Lightbulb</Text>
				  <Value>lightbulb11</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Note</Text>
				  <Value>note55</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Smiley</Text>
				  <Value>smiling36</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Soccer</Text>
				  <Value>soccer19</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Thumbs_Up</Text>
				  <Value>thumb52</Value>
			  </DropDownOption>
			  <DropDownOption>
				  <Text>Snowflake</Text>
				  <Value>winter35</Value>
			  </DropDownOption>
		  </Options>
	  </TemplateCaptionSetting>
  </TemplateCaptions>
  <SlideCaptions></SlideCaptions>
  <SlideImageCountDefinitions>
    <SlideImageCountDefinition>
      <MinimumImageCount>8</MinimumImageCount>
      <MaximumImageCount>8</MaximumImageCount>
    </SlideImageCountDefinition>
  </SlideImageCountDefinitions>
  <DefaultAudioInfo>
    <AudioUrl></AudioUrl>
    <Id></Id>
    <Length>0</Length>
    <AudioType>NoAudio</AudioType>
    <Name></Name>
  </DefaultAudioInfo>
  <AudioProviderSettings>
    <WebServiceRoot>https://ws.flixpress.com/AudioWebService.asmx?WSDL</WebServiceRoot>
    <ResultsPageSize>12</ResultsPageSize>
    <GetOperationName>GetAudio</GetOperationName>
    <SearchOperationName>SearchAudio</SearchOperationName>
    <GetCategoryTreeOperationName>GetCategoryTree</GetCategoryTreeOperationName>
  </AudioProviderSettings>
  <AudioDisplaySettings>
    <ThumbnailsRoot>https://fpsound.s3.amazonaws.com/</ThumbnailsRoot>
    <FullViewRoot>https://fpsound.s3.amazonaws.com/</FullViewRoot>
    <ThumbnailDimensions>
      <Width>52</Width>
      <Height>39</Height>
    </ThumbnailDimensions>
    <FullViewDimensions>
      <Width>400</Width>
      <Height>25</Height>
    </FullViewDimensions>
  </AudioDisplaySettings>
  <CustomAudioProviderSettings>
    <WebServiceRoot>https://ws.flixpress.com/CustomAudioWebService.asmx?WSDL</WebServiceRoot>
    <ResultsPageSize>12</ResultsPageSize>
    <GetOperationName>GetCustomAudio</GetOperationName>
    <NoRecordsMessage>You do not have any custom Audio in your account. Please upload your custom Audio.</NoRecordsMessage>
  </CustomAudioProviderSettings>
  <CustomAudioDisplaySettings>
    <ThumbnailsRoot>https://files.flixpress.com/CustomAudio/</ThumbnailsRoot>
    <FullViewRoot>https://files.flixpress.com/CustomAudio/</FullViewRoot>
    <ThumbnailDimensions>
      <Width>52</Width>
      <Height>39</Height>
    </ThumbnailDimensions>
    <FullViewDimensions>
      <Width>400</Width>
      <Height>25</Height>
    </FullViewDimensions>
  </CustomAudioDisplaySettings>
</TConfig>
